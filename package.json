{
  "name": "soulful-sessions",
  "version": "1.0.0",
  "description": "Chrome Extension that combines productivity and fun game",
  "scripts": {
    "build": "node build.js",
    "build:types": "tsc --noEmit",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration.test.ts",
    "test:integration": "jest integration.test.ts",
    "dev-screen": "node scripts/dev-screen-server.js",
    "package": "npm run build && node package-extension.js"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.270",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.10.1",
    "@types/puppeteer": "^5.4.7",
    "esbuild": "^0.27.0",
    "fast-check": "^4.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "puppeteer": "^24.31.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
